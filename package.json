{
  "name": "electron-music-player",
  "version": "1.0.0",
  "description": "音乐播放器",
  "main": "main.js",
  "scripts": {
    "dev": "nodemon --watch main.js --exec electron .",
    "start": "electron .",
    "packager": "electron-packager ./ electron-music-player --platform win32 --arch x64 --out OutApp --electron-version 4.0.1 --overwrite --icon=./icon.ico --version-string.CompanyName=Sogrey --version-string.ProductName=electron-music-player",
    "setup": "electron-installer-windows --src OutApp/electron-music-player-win32-x64/ --dest OutApp/installers/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sogrey/electron-music-player.git"
  },
  "author": "Sogrey",
  "license": "MIT",
  "keywords": [
    "electron",
    "windows",
    "music-player"
  ],
  "bugs": {
    "url": "https://github.com/Sogrey/electron-music-player/issues"
  },
  "homepage": "https://github.com/Sogrey/electron-music-player#readme",
  "devDependencies": {
    "electron": "^5.0.6",
    "electron-installer-windows": "^2.0.0",
    "electron-packager": "^13.0.1",
    "electron-log": "^3.0.6",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "electron-json-storage": "^4.1.6",
    "electron-positioner": "^4.1.0",
    "bootstrap": "^4.3.1",
    "electron-store": "^4.0.0"
  }
}